{
  "name": "heimrichhannot/contao-parallaximage-bundle",
  "type": "contao-bundle",
  "description": "A bundle to add parallax background images to articles.",
  "license": "LGPL-3.0-or-later",
  "require": {
    "php": "^7.1 || ^8.0",
    "contao/manager-bundle": "^4.4",
    "heimrichhannot/contao-utils-bundle": "^2.30"
  },
  "autoload": {
    "psr-4": {
      "HeimrichHannot\\ContaoParallaxImageBundle\\": "src/"
    },
    "classmap": [
      "src/"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/dca/",
      "src/Resources/contao/languages/",
      "src/Resources/contao/templates/"
    ]
  },
  "extra": {
    "contao-manager-plugin": "HeimrichHannot\\ContaoParallaxImageBundle\\ContaoManager\\Plugin"
  }
}
